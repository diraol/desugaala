{% load compressed %}

doctype 5
html
  head
    title DesuGaala 2012
    link(rel='stylesheet', href='{{ STATIC_URL }}css/bootstrap.min.css')
    link(rel='stylesheet', href='{{ STATIC_URL }}css/ui-lightness/jquery-ui-1.9.2.custom.min.css')
    {% compressed_css 'default' %}
    script
      window.desugaalaCsrfToken='{{ csrf_token }}';
    script(src='{{ STATIC_URL }}js/jquery-1.8.3.min.js')
    script(src='{{ STATIC_URL }}js/jquery-ui-1.9.2.custom.min.js')
    script(src='{{ STATIC_URL }}js/bootstrap.min.js')
    script(src='{{ STATIC_URL }}js/lodash.min.js')
    script(src='{{ STATIC_URL }}js/Bacon.js')
    script(src='{{ STATIC_URL }}js/Bacon.ajax.js')
    {% compressed_js 'default' %}

  body
    div.navbar.navbar-inverse.navbar-fixed-top
      div.navbar-inner
        div.container
          span.brand Urna Poli
          ul.nav
            li: a(href='http://desucon.fi/frostbite') Takaisin Desucon Frostbiten sivuille
    
    div.container.content
      block content

    div.container.footer
      p.muted.credit
        | Bruno Bandeira &middot; Diego Rabatone &middot; Guilherme Zampieri
      p.muted.credit
        | Â© 2013 &middot; Alguns direitos reservados:
